<h1>Sign up</h1>
<%= @user.errors.full_messages.join(", ") %>
<br>
<br>
<%= form_for @user do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <br>
  <%= f.submit %>
<% end  %>
